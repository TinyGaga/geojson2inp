{
  "name": "@watergis/geojson2inp",
  "version": "1.0.0",
  "description": "This module converts GeoJSON to EPANET INP file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "es6/index.js",
  "files": [
    "dist",
    "es6"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist",
    "build:esm": "tsc --project . --module es2015 --outDir ./es6",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watergis/geojson2inp.git"
  },
  "author": "Jin IGARASHI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watergis/geojson2inp/issues"
  },
  "homepage": "https://github.com/watergis/geojson2inp#readme",
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "fs": "0.0.1-security",
    "pad": "^3.2.0",
    "round-to": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^#/(.+)": "<rootDir>/src/$1"
    }
  }
}
